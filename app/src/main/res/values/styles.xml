<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" parent="@android:style/Theme.NoTitleBar.Fullscreen">
    </style>
    <style name="AppTheme" parent="@style/AppBaseTheme">
    </style>
    <style name="AppTheme.SplashScreen" parent="@style/AppTheme">
        <item name="android:windowBackground">@drawable/splash_screen</item>
    </style>
    <style name="Base">
        <item name="android:textColorLink">@color/xbid_white_primary</item>
    </style>
    <style name="Body" parent="@style/Base">
        <item name="android:textColor">@color/xbid_white_primary</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Body.BottomButton" parent="@style/Body.TitleComment">
        <item name="android:background">@drawable/xbid_button_background</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Body.GamerTag" parent="@style/Body">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.SectionText" parent="@style/Body">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/xbid_white_secondary</item>
    </style>
    <style name="Body.SectionTitle" parent="@style/Body">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.SuggestionText" parent="@style/Body.TextInputComment">
    </style>
    <style name="Body.TextInput" parent="@style/Body">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
        <item name="android:textColorHint">@color/xbid_white_secondary</item>
        <item name="android:background">@color/xbid_button_background</item>
        <item name="android:padding">3dp</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Body.TextInputComment" parent="@style/Body.TitleComment">
    </style>
    <style name="Body.Title" parent="@style/Body">
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.TitleComment" parent="@style/Body">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/xbid_white_secondary</item>
    </style>
    <style name="Body.UserName" parent="@style/Body">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="ButtonSubText_grey" parent="@style/SUI_base">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white_60_percent</item>
    </style>
    <style name="Error" parent="@style/Body">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Error.Button" parent="@style/Error">
        <item name="android:background">@drawable/xbid_button_background</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Error.Greeting" parent="@style/Error">
    </style>
    <style name="Error.Normal" parent="@style/Error">
    </style>
    <style name="FriendFinder_Title" parent="@style/SUI_white">
        <item name="android:textSize">24sp</item>
    </style>
    <style name="Header" parent="@style/Base">
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Header.Email" parent="@style/Header">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/xbid_white_secondary</item>
    </style>
    <style name="Header.UserName" parent="@style/Header">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="OnlineidUiTheme" parent="@android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:windowBackground">@color/xbid_activity_background</item>
    </style>
    <style name="RadioButtonSubText" parent="@style/SUI_base">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/textSoftWhite</item>
    </style>
    <style name="RadioButtonText" parent="@style/SUI_base">
        <item name="android:textSize">@dimen/settingsRadioButtonTextSize</item>
        <item name="android:textColor">@color/XboxWhite</item>
    </style>
    <style name="SUI_SB_base">
        <item name="typefaceSource">fonts/SegoeWP-Semibold.ttf</item>
    </style>
    <style name="SUI_base">
        <item name="android:ellipsize">end</item>
        <item name="typefaceSource">fonts/SegoeWP.ttf</item>
    </style>
    <style name="SUI_white" parent="@style/SUI_base">
        <item name="android:textColor">@color/textSoftWhite</item>
    </style>
    <style name="TcuiDialog" parent="@android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowAnimationStyle">@style/TcuiDialogAnimations</item>
    </style>
    <style name="TcuiDialogAnimations">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="TextAppearance.Compat.Notification" parent="@android:style/TextAppearance.Material.Notification">
    </style>
    <style name="TextAppearance.Compat.Notification.Info" parent="@android:style/TextAppearance.Material.Notification.Info">
    </style>
    <style name="TextAppearance.Compat.Notification.Info.Media" parent="@style/TextAppearance.Compat.Notification.Info">
        <item name="android:textColor">@color/secondary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Compat.Notification.Line2" parent="@style/TextAppearance.Compat.Notification.Info">
    </style>
    <style name="TextAppearance.Compat.Notification.Line2.Media" parent="@style/TextAppearance.Compat.Notification.Info.Media">
    </style>
    <style name="TextAppearance.Compat.Notification.Media" parent="@style/TextAppearance.Compat.Notification">
        <item name="android:textColor">@color/secondary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Compat.Notification.Time" parent="@android:style/TextAppearance.Material.Notification.Time">
    </style>
    <style name="TextAppearance.Compat.Notification.Time.Media" parent="@style/TextAppearance.Compat.Notification.Time">
        <item name="android:textColor">@color/secondary_text_default_material_dark</item>
    </style>
    <style name="TextAppearance.Compat.Notification.Title" parent="@android:style/TextAppearance.Material.Notification.Title">
    </style>
    <style name="TextAppearance.Compat.Notification.Title.Media" parent="@style/TextAppearance.Compat.Notification.Title">
        <item name="android:textColor">@color/primary_text_default_material_dark</item>
    </style>
    <style name="Widget.Support.CoordinatorLayout" parent="@android:style/Widget">
        <item name="statusBarBackground">#ff000000</item>
    </style>
    <style name="XboxGreen_RadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
    </style>
    <style name="friendfinder_button">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/xleiconbutton_textcolor_states</item>
        <item name="android:gravity">left</item>
        <item name="android:background">@drawable/friendfinder_button_border</item>
        <item name="android:padding">5dp</item>
        <item name="android:focusable">true</item>
        <item name="iconSymbolSize">18dp</item>
        <item name="iconSymbolTypefaceSource">fonts/SegXboxSymbol.ttf</item>
        <item name="typefaceSource">fonts/SegoeWP.ttf</item>
    </style>
    <style name="settings_checkbox_text" parent="@style/SUI_SB_base">
        <item name="android:textSize">@dimen/settingsCheckboxTextSize</item>
        <item name="android:textColor">@color/XboxGray6</item>
    </style>
    <style name="standard_subText" parent="@style/standard_text">
        <item name="android:textColor">@color/white_60_percent</item>
    </style>
    <style name="standard_text" parent="@style/SUI_white">
        <item name="android:textSize">16sp</item>
    </style>
    <style name="xbox_symbol">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/textSoftWhite</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="typefaceSource">fonts/SegXboxSymbol.ttf</item>
    </style>
    <style name="xle_button">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/xleiconbutton_textcolor_states</item>
        <item name="android:background">@drawable/xleiconbutton_background_states</item>
        <item name="iconSymbolSize">18dp</item>
        <item name="iconSymbolTypefaceSource">fonts/SegXboxSymbol.ttf</item>
        <item name="typefaceSource">fonts/SegoeWP.ttf</item>
    </style>
	
    <!-- NativeAd BEGIN -->
    <style name="AppTheme.AdAttribution">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">12sp</item>
        <item name="android:text">@string/ad_attribution</item>
        <item name="android:background">#FFCC66</item>
        <item name="android:width">15dp</item>
        <item name="android:height">15dp</item>
    </style>
    <!-- NativeAd END -->
	
</resources>
